{
  "name": "unsplash",
  "version": "1.1.1",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "manifest": "src/manifest.json",
    "main": "unsplash.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.7",
    "eslint": "^6.5.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "lodash": "^4.17.15",
    "serialize-javascript": ">=2.1.1"
  },
  "author": "Sketch",
  "repository": "https://github.com/sketch-hq/unsplash-sketchplugin.git",
  "description": "Easily grab images from Unsplash",
  "license": "MIT",
  "dependencies": {
    "@skpm/buffer": "^0.1.4",
    "@skpm/fs": "^0.2.6",
    "sketch-image-downloader": "^1.0.1"
  }
}
